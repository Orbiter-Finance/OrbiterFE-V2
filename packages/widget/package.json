{
  "name": "@orbiter-finance/widget",
  "version": "0.0.3",
  "private": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "build:css": "npx tailwindcss -i ./src/index.css -o ./dist/index.css",
    "build": "tsup src/index.ts --format cjs,esm && tsc --emitDeclarationOnly --outDir dist && npm run build:css && npm run postbuild",
    "postbuild": "cp package.json dist/",
    "dev": "tsup --watch"
  },
  "devDependencies": {
    "@turbo/gen": "^1.12.4",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "tailwindcss": "^3.4.13",
    "tsup": "^8.2.4",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@orbiter-finance/blockchain-gas": "workspace:^",
    "@orbiter-finance/bridge-sdk": "workspace:*",
    "@orbiter-finance/eslint-config": "workspace:*",
    "@orbiter-finance/explore-link": "workspace:*",
    "@orbiter-finance/typescript-config": "workspace:*",
    "@orbiter-finance/ui": "workspace:*",
    "@orbiter-finance/wallet-balance": "workspace:*",
    "@orbiter-finance/wallet-management": "workspace:*",
    "@tanstack/react-query": "^5.32.0",
    "autoprefixer": "^10.4.19",
    "bignumber.js": "^9.1.2",
    "clsx": "^2.1.1",
    "copy-to-clipboard": "^3.3.3",
    "dayjs": "^1.11.10",
    "esbuild-plugin-inline-image": "^0.0.9",
    "ethereum-blockies": "^0.1.1",
    "ethers": "^6.13.2",
    "jotai": "^2.9.3",
    "lucide-react": "^0.439.0",
    "node-polyfill-webpack-plugin": "^3.0.0",
    "postcss": "^8",
    "postcss-import": "^16.1.0",
    "react": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "tailwind-merge": "^2.5.2",
    "tslib": "^2.7.0",
    "viem": "^2.21.14",
    "webpack": "^5.91.0"
  }
}